    "built-proyect": "concurrently \"npm run dev-backend --prefix backend\" \"npm run start-frontend --prefix frontend\""
  },
  "dependencies": {
    "gsap": "^3.12.4",
    "jwt-decode": "^4.0.0",
    "concurrently": "^7.0.0"
  }
